{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Astralform",
  "colors": {
    "primary": "#667eea",
    "light": "#818cf8",
    "dark": "#764ba2"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "iOS SDK",
            "pages": [
              "ios-sdk/installation",
              "ios-sdk/configuration",
              "ios-sdk/conversations",
              "ios-sdk/client-tools"
            ]
          },
          {
            "group": "Dashboard",
            "pages": [
              "dashboard/projects",
              "dashboard/api-keys",
              "dashboard/llm-providers",
              "dashboard/mcp-servers",
              "dashboard/platform-tools"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Chat",
            "pages": [
              "api-reference/chat/stream",
              "api-reference/chat/tool-result"
            ]
          },
          {
            "group": "Conversations",
            "pages": [
              "api-reference/conversations/list",
              "api-reference/conversations/get",
              "api-reference/conversations/delete"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://astralform.ai",
          "icon": "browser"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/astralform-ai",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@astralform.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://astralform.ai"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/astralform-ai"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "api": {
    "baseUrl": "https://api.astralform.ai"
  }
}
